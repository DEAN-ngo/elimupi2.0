server {
	listen 		80;
	listen 		[::]:80;
	listen		443 ssl;
	server_name fdroid.elimupi.online;
	
	# SSL settings
    ssl_certificate     /etc/nginx/certs/elimupi_online.cert.pem;
    ssl_certificate_key /etc/nginx/certs/elimupi_online.key.pem;
    ssl_protocols       TLSv1 TLSv1.1 TLSv1.2;
    ssl_ciphers         HIGH:!aNULL:!MD5;
	error_log 			/var/www/log/fdroid-log;
	location / {
		autoindex on;
		root /mnt/content/fdroid;
	}
}


